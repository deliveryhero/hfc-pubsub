{
  "name": "@honestfoodcompany/pubsub",
  "version": "1.4.1",
  "description": "pubsub framework for managing topics and subscriptions",
  "main": "dist/index.js",
  "scripts": {
    "watch": "nodemon --watch 'src/**/*' -e ts --exec yarn run build",
    "test": "jest --coverage",
    "build": "tsc",
    "prepare": "npm run build",
    "lint": "tslint -p tsconfig.json",
    "subscriptions": "ts-node src/cli/subscriptions.ts",
    "postinstall": "npm run build && node postinstall.js"
  },
  "keywords": [
    "google pubsub",
    "pubsub",
    "publish/subscribe",
    "subscriptions",
    "pubsub framework"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "typescript": "^3.7.4",
    "@google-cloud/pubsub": "^1.1.0",
    "@types/wrap-ansi": "^3.0.0",
    "chalk": "^2.4.2",
    "cli-ux": "^5.3.2",
    "dotenv": "^8.1.0",
    "find-config": "^1.0.0",
    "fs": "0.0.1-security",
    "humanize-duration": "^3.20.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mongoose": "^5.7.3",
    "nodemon": "^1.19.3",
    "wrap-ansi": "^6.0.0",
    "yargs": "^14.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.12.6",
    "@types/yargs": "^13.0.2",
    "@typescript-eslint/eslint-plugin": "2.17.0",
    "@typescript-eslint/parser": "2.17.0",
    "@typescript-eslint/typescript-estree": "2.17.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "lint-staged": "8.1.7",
    "prettier": "^1.19.1",
    "replace-in-file": "^4.1.3",
    "ts-jest": "^24.1.0",
    "ts-node": "8.1.0",
    "tslint": "5.16.0",
    "typescript": "^3.4.5",
    "yarn": "^1.17.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Anthony Martin",
  "repository": {
    "type": "git",
    "url": "https://anthonydmartin@bitbucket.org/anthonydmartin/hfc-pubsub.git"
  },
  "license": "MIT",
  "bin": {
    "subscriptions": "./dist/cli/subscriptions.js"
  },
  "husky": {
    "hooks": {
      "post-commit": "(git-branch-is develop && HUSKY_SKIP_HOOKS=1 yarn version --patch)"
    }
  }
}
